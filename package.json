{
  "name": "familie-ef-mikrofrontend-minside",
  "version": "2.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --port 3000",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "prepare": "husky install",
    "lint": "eslint --fix --ext .ts,.tsx,.js,.jsx .",
    "pretty-quick": "pretty-quick --pattern \"**/*.*(ts|tsx)\""
  },
  "dependencies": {
    "@amplitude/analytics-browser": "^1.10.4",
    "@navikt/aksel-icons": "^5.12.0",
    "@navikt/ds-css": "^5.12.0",
    "@navikt/ds-react": "^5.12.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "swr": "^2.2.5"
  },
  "devDependencies": {
    "@rollup/plugin-terser": "^0.4.4",
    "@testing-library/react": "13.4.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/react": "^18.2.57",
    "@types/react-dom": "^18.2.19",
    "@types/react-modal": "^3.16.3",
    "@typescript-eslint/eslint-plugin": "5.50.0",
    "@typescript-eslint/parser": "5.50.0",
    "@vitejs/plugin-react": "3.1.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "8.6.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.33.2",
    "husky": "7.0.0",
    "jsdom": "21.1.0",
    "lint-staged": "13.1.0",
    "prettier": "2.8.3",
    "pretty-quick": "3.1.3",
    "rollup-plugin-import-map": "3.0.0",
    "typescript": "4.9.5",
    "vite": "4.5.1",
    "vite-plugin-css-injected-by-js": "2.4.0",
    "vite-plugin-mock": "2.9.6",
    "vitest": "0.28.3"
  },
  "prettier": {
    "printWidth": 120
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint -c .eslintrc.pre-commit.cjs --fix",
      "prettier --cache --write"
    ],
    "*.{css,md,html,json}": [
      "prettier --cache --write"
    ]
  }
}
